<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml" fx:controller="clientapp.controllers.BoardController">
    <Pane fx:id="ResultPane" visible="false">
        <Label fx:id="ResultLabel" layoutX="50" layoutY="20" />
    </Pane>
    <GridPane layoutX="50" layoutY="100">
        <Button fx:id="Button1" onAction="#handleButtonClick" text="" />
        <Button fx:id="Button2" onAction="#handleButtonClick" text="" />
        <Button fx:id="Button3" onAction="#handleButtonClick" text="" />
        <Button fx:id="Button4" onAction="#handleButtonClick" text="" />
        <Button fx:id="Button5" onAction="#handleButtonClick" text="" />
        <Button fx:id="Button6" onAction="#handleButtonClick" text="" />
        <Button fx:id="Button7" onAction="#handleButtonClick" text="" />
        <Button fx:id="Button8" onAction="#handleButtonClick" text="" />
        <Button fx:id="Button9" onAction="#handleButtonClick" text="" />
    </GridPane>
    <Line fx:id="Row1" startX="10" startY="50" endX="250" endY="50" visible="false" />
    <Line fx:id="Row2" startX="10" startY="150" endX="250" endY="150" visible="false" />
    <Line fx:id="Row3" startX="10" startY="250" endX="250" endY="250" visible="false" />
    <Line fx:id="Col1" startX="50" startY="10" endX="50" endY="250" visible="false" />
    <Line fx:id="Col2" startX="150" startY="10" endX="150" endY="250" visible="false" />
    <Line fx:id="Col3" startX="250" startY="10" endX="250" endY="250" visible="false" />
    <Line fx:id="Diagonal1" startX="10" startY="10" endX="250" endY="250" visible="false" />
    <Line fx:id="Diagonal2" startX="250" startY="10" endX="10" endY="250" visible="false" />
</AnchorPane>
